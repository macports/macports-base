    {checkSimpleArgs 1 1 {
	checkWord
    }}
pcx::check 0.7 std ::textutil::trim::trimPrefix \
    {checkSimpleArgs 2 2 {
	checkWord
	checkWord
    }}
pcx::check 0.7 std ::textutil::trim::trimleft \
    {checkSimpleArgs 1 2 {
	checkWord
	checkRegexp
    }}
pcx::check 0.7 std ::textutil::trim::trimright \
    {checkSimpleArgs 1 2 {
	checkWord
	checkRegexp
    }}

