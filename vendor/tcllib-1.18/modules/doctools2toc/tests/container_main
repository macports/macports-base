test doctools-toc-${impl}-30.0 {title, default} -setup {
    doctools::toc I
} -body {
    I title
} -cleanup {
    I destroy
} -result {}

test doctools-toc-${impl}-30.1 {title, set} -setup {
    doctools::toc I
} -body {
    I title T
} -cleanup {
    I destroy
} -result T

